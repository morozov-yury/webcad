    <template name="notification.event.contest.canceled.competitor">
        <locale language="en">
            <title>Contest canceled</title>
            <body>
				<div location='TYPE|contest_link|ID|${contest.id}|STYLE|link'></div> canceled due to ${message}.
            </body>
        </locale>
        <locale language="ru">
            <title>Отмена конкурса</title>
            <body>
            	<div location='TYPE|contest_link|ID|${contest.id}|STYLE|link'></div> отменен по причине ${message}.
            </body>
        </locale>
    </template>
